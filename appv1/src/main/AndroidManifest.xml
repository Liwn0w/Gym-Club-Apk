<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.bjtu.example.gym_club">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login_page" />
        <activity android:name=".sign_page" />
        <activity android:name=".profile" />
        <activity android:name=".find" />
        <activity android:name=".news" />
        <activity android:name=".schedule" />
        <activity android:name=".diary" />
        <activity android:name=".settings_page"></activity>
    </application>

</manifest>